{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
  "esModuleInterop": true,
	"jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
